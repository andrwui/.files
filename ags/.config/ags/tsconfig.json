{
  "compilerOptions": {
    "target": "es2017",
    "module": "Preserve",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "lib": [
      "ES2022"
    ],
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "noImplicitAny": false,
    "outDir": "/tmp/ags/js",
    "baseUrl": ".",
    "typeRoots": [
      "./types/*"
    ],
    "allowImportingTsExtensions": true,
    "skipLibCheck": true,
      "allowSyntheticDefaultImports": true
  }
}